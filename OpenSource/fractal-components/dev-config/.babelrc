{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%"
        }
      }
    ]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-runtime",
    "async-to-promises",
    "transform-flow-strip-types"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ],
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "local": {
      "presets": [
        "es2015"
      ]
    }
  }
}